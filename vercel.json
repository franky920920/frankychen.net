{
	"cleanUrls": true,
	"headers": [
		{
			"source": "/(.*)",
			"headers": [
				{
					"key": "Strict-Transport-Security",
					"value": "max-age=63072000; includeSubDomains; preload"
				},
				{
					"key": "X-Frame-Options",
					"value": "DENY"
				},
				{
					"key": "x-content-type-options",
					"value": "nosniff"
				},
				{
					"key": "x-frame-options",
					"value": "SAMEORIGIN"
				},
				{
					"key": "cache-control",
          			"value": "public, max-age=1800, stale-while-revalidate=18000"
          		}
			]
		}
	]
}
